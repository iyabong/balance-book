		<Exec Command="robocopy &quot;$(PublishUrl)&quot; &quot;$(SolutionDir)frontend\public\resume&quot; /E /NFL /NDL /NJH /NJS"
			  IgnoreExitCode="true" />
	</Target>-->

	<!--<Target Name="CopyToFrontend" AfterTargets="Publish">
		--><!-- Vercel용 public/resume 폴더 없으면 생성 --><!--
		<MakeDir Directories="$(SolutionDir)frontend\public\resume"
				 Condition="!Exists('$(SolutionDir)frontend\public\resume')" />

		--><!-- 게시 산출물 루트에서 그대로 복사 --><!--
		<Exec Command='robocopy "$(PublishDir)." "$(SolutionDir)frontend\public\resume" /E /MIR /NFL /NDL /NJH /NJS'
			  IgnoreExitCode="true" />
	</Target>-->

	<Target Name="CopyToFrontend" AfterTargets="Publish">
		<!-- public/resume 폴더 보장 -->
		<MakeDir Directories="$(SolutionDir)frontend\public\resume"
				 Condition="!Exists('$(SolutionDir)frontend\public\resume')" />
		<!-- 중요: 원본을 $(PublishDir)\wwwroot\. 로 지정 (끝에 \. 필수) -->
		<Exec Command='robocopy "$(PublishDir)wwwroot\." "$(SolutionDir)frontend\public" /MIR /NFL /NDL /NJH /NJS'
			  IgnoreExitCode="true" />
	</Target>
</Project>